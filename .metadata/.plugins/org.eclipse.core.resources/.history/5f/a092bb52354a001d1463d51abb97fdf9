public class DesBinarySearch {

	public static void main(String[] args) {
		int[] array = {20,17,15,14,13,12,10,7,8,4};
		int key = 4;
		int last = array.length - 1;
		int first = 0;
		
		System.out.println(Arrays.toString(array)+" "+key+" "+last+" "+first);

	}

}
